    "@nestjs/schedule": "^4.0.1",
    "@prisma/client": "^5.9.1",
    "apify-client": "^2.12.0",
    "axios": "^1.6.0",
    "cities.json": "^1.1.43",
    "class-transformer": "^0.5.1",

  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:devng": "./start-app.sh",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start:ngrok": "ngrok http --url=pug-simple-tadpole.ngrok-free.app 3000",
    "generate:dev": "npx prisma generate --schema ./prisma/schema.prisma",
    "migrate:dev": "npx prisma migrate dev --name init",
    "seed": "npx prisma db seed"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.722.0",
    "@aws-sdk/s3-request-presigner": "^3.722.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.2.5",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.5",

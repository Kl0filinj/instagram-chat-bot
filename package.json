    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^5.9.1",
    "puppeteer": "^24.2.1",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
